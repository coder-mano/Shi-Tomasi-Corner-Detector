def hconcat(src, dst=None): # real signature unknown; restored from __doc__
    """
    hconcat(src[, dst]) -> dst
    .   @overload
    .    @code{.cpp}
    .       std::vector<cv::Mat> matrices = { cv::Mat(4, 1, CV_8UC1, cv::Scalar(1)),
    .                                         cv::Mat(4, 1, CV_8UC1, cv::Scalar(2)),
    .                                         cv::Mat(4, 1, CV_8UC1, cv::Scalar(3)),};
